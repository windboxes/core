{
  "name": "@windboxes/core",
  "description": "WindBoxes processor core",
  "version": "0.0.1-beta.8.35",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "types/index.d.ts",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^17.0.26",
    "tsup": "^6.3.0",
    "typescript": "^4.8.4"
  },
  "scripts": {
    "test": "ts-node ./src/test.ts",
    "buildOld": "ts-node ./esbuild",
    "build": "yarn build:dist && yarn build:declarations",
    "build:dist": "tsup --format cjs,esm",
    "build:declarations": "tsc --emitDeclarationOnly --declaration --outDir types"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "splitting": false,
    "sourcemap": true,
    "clean": true
  },
  "repository": "https://github.com/windboxes/core.git",
  "author": "SnowFireWolf",
  "keywords": [
    "windboxes",
    "wind",
    "boxes",
    "typescript",
    "css",
    "css module",
    "array"
  ],
  "publishConfig": {
    "@snowfirewolf:registry": "https://npm.pkg.github.com"
  },
  "dependencies": {}
}
